generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Commit {
  id         String   @id
  created_on DateTime
  email      String
  repo_name  String
  branch     String
  Student    Student  @relation(fields: [email], references: [email])
  Repo       Repo     @relation(fields: [repo_name], references: [name])
}

model Repo {
  name        String    @id @unique
  forked_date DateTime?
  Commit      Commit[]
}

model Student {
  name       String
  email      String   @id @unique
  is_student Boolean
  Commit     Commit[]
}
