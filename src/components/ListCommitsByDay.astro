--- 
const {pivotTable, studentNames} = Astro.props 
---

<table>
  <thead>
    <tr>
      <th>Repo/Student</th>
        {studentNames.map(name => <th>${name}</th>)}
    </tr>
  </thead>
  <tbody>
    {Object.entries(pivotTable).map(([repoName, commitsByStudent]) => (<tr>
        <td>{repoName}</td>
        {studentNames.map(studentName => <td>{commitsByStudent[studentName] || ''}</td>)}
</tr>))}
  </tbody>
</table>
